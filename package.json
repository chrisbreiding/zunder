  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "zunder": "dist/lib/zunder.js"
  },
  "dependencies": {
    "@cypress/coffeeify": "^1.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.0",
    "browserify": "^13.0.0",
    "browserify-resolutions": "^1.1.0",
    "chalk": "^1.1.3",
    "del": "^2.2.0",
    "envify": "^4.1.0",
    "express": "^4.13.4",
    "fancy-log": "^1.3.0",
    "fs-extra": "^4.0.0",
    "glob": "^7.0.3",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.6",
    "gulp-compile-handlebars": "^0.6.1",
    "gulp-custom-filter": "^0.2.5",
    "gulp-if": "^2.0.1",
    "gulp-nodemon": "^2.0.6",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.0.0",
    "gulp-rev-delete-original": "^0.2.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^2.3.1",
    "gulp-spawn-mocha": "^3.0.0",
    "gulp-stylus": "^2.4.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.5",
    "lodash": "^4.11.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.1",
    "morgan": "^1.7.0",
    "node-sass-globbing": "0.0.23",
    "portfinder": "^1.0.3",
    "stream-to-promise": "^2.2.0",
    "through": "^2.3.8",
    "undertaker": "^0.15.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-fs": "^2.4.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^4.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^5.0.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
    "build": "babel src --out-dir dist --ignore scaffold/*",
    "clean": "rm -rf dist && mkdir dist",
    "copy-scaffold": "mkdir -p dist/scaffold; cp -R src/scaffold dist/scaffold",
    "copy-static": "mkdir -p dist/lib; cp -R src/lib/*.hbs dist/lib/",
    "copy": "npm run copy-scaffold; npm run copy-static",
    "prepublish": "npm run clean; npm run build; npm run copy",
    "watch-babel": "babel src --watch --out-dir dist --ignore scaffold/*",
    "watch-static": "chokidar 'src/lib/*.hbs' -c 'npm run copy-static'",
    "start": "npm run copy; concurrently --kill-others --names 'babel: ,static:' --prefix '{name}' 'npm run watch-babel' 'npm run watch-static'"
  }
}
