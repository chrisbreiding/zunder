  "homepage": "https://github.com/chrisbreiding/zunder",
  "repository": "git://github.com/chrisbreiding/zunder.git",
  "author": "Chris Breiding (http://www.chrisbreiding.com/)",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "zunder": "dist/lib/zunder.js"
  },
  "dependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@cypress/coffeeify": "1.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babelify": "9.0.0",
    "body-parser": "1.18.3",
    "browserify": "16.2.2",
    "browserify-resolutions": "1.1.0",
    "chalk": "2.4.1",
    "del": "3.0.0",
    "envify": "4.1.0",
    "express": "4.16.3",
    "fancy-log": "1.3.2",
    "fs-extra": "7.0.0",
    "glob": "7.1.3",
    "gulp-autoprefixer": "6.0.0",
    "gulp-babel": "8.0.0",
    "gulp-babel-minify": "0.4.3",
    "gulp-clean-css": "3.10.0",
    "gulp-compile-handlebars": "0.6.1",
    "gulp-custom-filter": "0.2.5",
    "gulp-if": "2.0.2",
    "gulp-nodemon": "2.2.1",
    "gulp-notify": "3.2.0",
    "gulp-plumber": "1.2.0",
    "gulp-rename": "1.4.0",
    "gulp-rev": "8.1.1",
    "gulp-rev-delete-original": "0.2.3",
    "gulp-rev-replace": "0.4.4",
    "gulp-sass": "4.0.1",
    "gulp-spawn-mocha": "5.0.1",
    "gulp-stylus": "2.7.0",
    "gulp-util": "3.0.8",
    "gulp-watch": "5.0.1",
    "istanbul": "0.4.5",
    "lodash": "4.17.10",
    "mkdirp": "0.5.1",
    "mocha": "5.2.0",
    "morgan": "1.9.0",
    "node-sass": "4.9.3",
    "node-sass-globbing": "0.0.23",
    "portfinder": "1.0.17",
    "stream-to-promise": "2.2.0",
    "through": "2.3.8",
    "tsify": "4.0.0",
    "typescript": "3.0.3",
    "undertaker": "1.2.0",
    "vinyl-buffer": "1.0.1",
    "vinyl-fs": "3.0.3",
    "vinyl-source-stream": "2.0.0",
    "watchify": "3.11.0",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "chokidar": "2.0.4",
    "chokidar-cli": "1.2.1",
    "concurrently": "4.0.1",
    "eslint": "5.5.0",
    "eslint-plugin-react": "7.11.1"
  },
  "scripts": {
    "build": "babel src --out-dir dist --ignore scaffold/*",
    "clean": "rm -rf dist && mkdir dist",
    "copy-scaffold": "mkdir -p dist/scaffold; cp -R src/scaffold dist/scaffold",
    "copy-static": "mkdir -p dist/lib; cp -R src/lib/*.hbs dist/lib/",
    "copy": "npm run copy-scaffold; npm run copy-static",
    "prepublish": "npm run clean; npm run build; npm run copy",
    "watch-babel": "babel src --watch --out-dir dist --ignore scaffold/*",
    "watch-static": "chokidar 'src/lib/*.hbs' -c 'npm run copy-static'",
    "start": "npm run copy; concurrently --kill-others --names 'babel: ,static:' --prefix '{name}' 'npm run watch-babel' 'npm run watch-static'"
  }
}
