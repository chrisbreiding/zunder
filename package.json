{
  "name": "zunder",
  "version": "0.4.0",
  "description": "A front-end build tool that makes developing apps with Browserify, CoffeeScript, and Stylus a breeze",
  "homepage": "https://github.com/chrisbreiding/zunder",
  "repository": "git://github.com/chrisbreiding/zunder.git",
  "author": "Chris Breiding (http://www.chrisbreiding.com/)",
  "license": "MIT",
  "main": "dist/index.js",
  "keywords": [
    "coffeescript",
    "stylus",
    "browserify"
  ],
  "dependencies": {
    "browserify": "^9.0.8",
    "coffeeify": "^1.1.0",
    "del": "^1.1.1",
    "event-stream": "^3.3.0",
    "express": "^4.12.3",
    "gulp-minify-css": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^3.0.1",
    "gulp-stylus": "^2.0.1",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.2.4",
    "handlebars": "^3.0.2",
    "lodash": "^3.7.0",
    "mkdirp": "^0.5.0",
    "portfinder": "^0.4.0",
    "rsvp": "^3.0.18",
    "through2": "^0.6.5",
    "uglifyify": "^3.0.1",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.1.2"
  },
  "devDependencies": {
    "chai": "~1.9.1",
    "coffee-script": "~1.7.1",
    "mocha": "^2.2.4",
    "proxyquire": "~1.0.1",
    "sinon": "~1.10.2",
    "sinon-chai": "~2.5.0"
  },
  "peerDependencies": {
    "gulp": ">=3.7.0"
  },
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register --require test/common --reporter spec",
    "prepublish": "coffee --compile --output dist/lib src/lib; coffee --compile --output dist/tasks src/tasks; coffee --compile --output dist/ src/index.coffee; cp -R src/scaffold/ dist/scaffold/"
